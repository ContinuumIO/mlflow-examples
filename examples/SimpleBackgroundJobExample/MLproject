name: SimpleBackgroundJobExample

entry_points:
  process_one:
    parameters:
      inbound: {type: string, default: "data/inbound"}
      outbound: {type: string, default: "data/outbound"}
    command: "python -m steps.process_one --inbound {inbound} --outbound {outbound}"

  main:
    parameters:
      inbound: {type: string, default: "data/inbound"}
      outbound: {type: string, default: "data/outbound"}
    command: "python -m steps.main --inbound {inbound} --outbound {outbound}"
